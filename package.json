{
  "name": "test-thy-api",
  "private": true,
  "version": "0.0.1",
  "description": "Experimenting with API test methods",
  "main": "index.js",
  "engines": {
    "node": ">= 10.x",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test:unit": "mocha ./tests/unit/**",
    "test:integration": "mocha ./tests/integration/**",
    "test:e2e": "node node_modules/.bin/cucumber-js -f node_modules/cucumber-pretty tests/cucumber/**",
    "test:schemas": "node tests/schemas/app.spec.js",
    "test:fuzz": "mocha ./tests/fuzz/**",
    "test:generative": "mocha ./tests/generative/**",
    "check:lint": "eslint --ignore-path .gitignore *.js",
    "check:auditAndOutdated": "npm run audit && npm outdated",
    "check:snyk": "snyk test --dev",
    "generate:postman": "node openapi.js",
    "generate:postman:cli": "openapi2postmanv2 -s swagger.json -o postman.collection"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenEdridge/Test-Thy-API.git"
  },
  "author": "Ben Edridge",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BenEdridge/Test-Thy-API/issues"
  },
  "homepage": "https://github.com/BenEdridge/Test-Thy-API#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.2",
    "dredd": "^11.2.15",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "fast-check": "^1.16.0",
    "jsverify": "^0.8.4",
    "lolex": "^4.1.0",
    "mocha": "^6.2.0",
    "nock": "^10.0.6",
    "sinon": "^7.3.2",
    "snyk": "^1.199.2",
    "supertest": "^4.0.2",
    "swagger-client": "^3.9.0",
    "swagger-ui": "^3.23.1",
    "testcheck": "^1.0.0-rc.2"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "express": "^4.17.1",
    "hooks": "^0.3.2",
    "newman": "^4.5.4",
    "openapi-to-postmanv2": "0.0.18",
    "postman-collection": "^3.5.3",
    "swagger-jsdoc": "^3.4.0"
  }
}
